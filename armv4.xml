<SPEC>
 <LANGUAGE>C</LANGUAGE>
 <TEMPLATE>armv4.c</TEMPLATE>
 <FUNCROOT>armv4_disassemble</FUNCROOT>
 <FUNCVARSDEF>unsigned int in, opcode* op</FUNCVARSDEF>
 <FUNCVARSCALL>in, op</FUNCVARSCALL>
 <FUNCUNDEF>armv4_undefined</FUNCUNDEF>
 <FUNCSTUB>armv4_stub</FUNCSTUB>
 <FUNCTABLE>armv4_table</FUNCTABLE>
 
 <MACROPATTERN name="shift">
  <ITEM>------1-------------------------</ITEM>
  <ITEM>------0--------------------0----</ITEM>
  <ITEM>------0-----------------0--1----</ITEM>
 </MACROPATTERN> 
 
 <MACROPATTERN name="cond">
  <ITEM>0000----------------------------</ITEM>
  <ITEM>0001----------------------------</ITEM>
  <ITEM>0010----------------------------</ITEM>
  <ITEM>0011----------------------------</ITEM>
  <ITEM>0100----------------------------</ITEM>
  <ITEM>0101----------------------------</ITEM>
  <ITEM>0110----------------------------</ITEM>
  <ITEM>0111----------------------------</ITEM>
  <ITEM>1000----------------------------</ITEM>
  <ITEM>1001----------------------------</ITEM>
  <ITEM>1010----------------------------</ITEM>
  <ITEM>1011----------------------------</ITEM>
  <ITEM>1100----------------------------</ITEM>
  <ITEM>1101----------------------------</ITEM>
  <ITEM>1110----------------------------</ITEM>
  <!-- <ITEM>1111----------------------------</ITEM> -->
 </MACROPATTERN> 
 
 <PATTERN label="armv4_dataprocess">----00-0000--------------------- | cond | shift</PATTERN> <!-- AND -->
 <PATTERN label="armv4_dataprocess">----00-0001--------------------- | cond | shift</PATTERN> <!-- EOR -->
 <PATTERN label="armv4_dataprocess">----00-0010--------------------- | cond | shift</PATTERN> <!-- SUB -->
 <PATTERN label="armv4_dataprocess">----00-0011--------------------- | cond | shift</PATTERN> <!-- RSB -->
 <PATTERN label="armv4_dataprocess">----00-0100--------------------- | cond | shift</PATTERN> <!-- ADD -->
 <PATTERN label="armv4_dataprocess">----00-0101--------------------- | cond | shift</PATTERN> <!-- ADC -->
 <PATTERN label="armv4_dataprocess">----00-0110--------------------- | cond | shift</PATTERN> <!-- SBC -->
 <PATTERN label="armv4_dataprocess">----00-0111--------------------- | cond | shift</PATTERN> <!-- RSC -->
 <PATTERN label="armv4_dataprocess">----00-10001-------------------- | cond | shift</PATTERN> <!-- TST -->
 <PATTERN label="armv4_dataprocess">----00-10011-------------------- | cond | shift</PATTERN> <!-- TEQ -->
 <PATTERN label="armv4_dataprocess">----00-10101-------------------- | cond | shift</PATTERN> <!-- CMP -->
 <PATTERN label="armv4_dataprocess">----00-10111-------------------- | cond | shift</PATTERN> <!-- CMN -->
 <PATTERN label="armv4_dataprocess">----00-1100--------------------- | cond | shift</PATTERN> <!-- ORR -->
 <PATTERN label="armv4_dataprocess">----00-1101--------------------- | cond | shift</PATTERN> <!-- MOV -->
 <PATTERN label="armv4_dataprocess">----00-1110--------------------- | cond | shift</PATTERN> <!-- BIC -->
 <PATTERN label="armv4_dataprocess">----00-1111--------------------- | cond | shift</PATTERN> <!-- MVN -->
</SPEC>
